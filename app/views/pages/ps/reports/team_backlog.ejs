<% include ../../../partials/header.ejs %>
  <% include ../../../partials/nav.ejs %>
    <main class="app-content">

      <div class="app-title">
        <div>
          <h1> <%= cftName.name %> Backlog</h1>
        </div>
        <ul class="app-breadcrumb breadcrumb">
          <li class="breadcrumb-item">
            <i class="fa fa-home fa-lg"></i>
          </li>
          <li class="breadcrumb-item">PS</li>
          <li class="breadcrumb-item">Team Goals</li>
          <li class="breadcrumb-item">Backlog</li>
          <li class="breadcrumb-item"><a href="/ps/reports/mrr/teams/<%= link_data.date.year %>/<%= link_data.date.month %>">MRR</a></li>
          <li class="breadcrumb-item active">
            <%= cftName.name %>
          </li>
        </ul>
      </div>
      <div class="row">
        <div class="col">
          <div class="tile">
              <table class="table" id="active_projects">
                <thead>
                  <tr>
                    <th scope="col">LBS ID</th>
                    <th scope="col">Location Name</th>
                    <th scope="col">Total MRR</th>
                    <th scope="col">Estimated Go Live Date</th>
                    <th scope="col">Project Type</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(var i=0; i < lbs.length; i++) { %>
                    <tr>
                     
                      <td>
                        <a href="https://system.na2.netsuite.com/app/accounting/project/project.nl?id=<%= lbs[i].id %>&whence=" target="_blank">
                        <%= lbs[i].id %>
                      </a>
                      </td>
                      <td>
                        <%= lbs[i].location_name %>
                      </td>
                      <td>
                        <%= lbs[i].total_mrr %>
                      </td>
                      <td>
                         <!-- activation Date -->
                        <%= moment(lbs[i].estimated_go_live).format( 'MMM-DD-YYYY') %>
                      </td>
                       <td>
                          <!-- activation Date -->
                         <%= lbs[i].project_type %>
                       </td>
                    </tr>
                    <% } %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <% include ../../../partials/footer.ejs %>
    </main>