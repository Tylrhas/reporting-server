<% include ../../../partials/header.ejs %>
<% include ../../../partials/nav.ejs %>
<main class="app-content">
 <div class="app-title">
  <div>
   <h1>Current CFT Workload</h1>
   <p>Project level WIP for teams</p>
  </div>
  <ul class="app-breadcrumb breadcrumb">
   <li class="breadcrumb-item">
    <i class="fa fa-home fa-lg"></i>
   </li>
   <li class="breadcrumb-item">PS</li>
   <li class="breadcrumb-item active">Team Workload </li>
  </ul>
 </div>
 <% for(var i = 0; i < pods.length; i++) { key = pods[i].id %>
 <% if ( i === 0 || i === 3) { %>
 <div class="row">
  <% } %>
  <div class="col-md-4">
   <div class="tile">
    <h4 class="center">
     <%= pods[i].name %>
    </h4>
    <table class="table center col-md-12 center">
     <thead>
      <tr>
       <th>Active Projects
       </th>
       <th>Scheduled Projects</th>
       <th>WIP Limit</th>
      </tr>
     </thead>
     <tbody>
      <tr>
       <td><%= activeProjects[key].count %>
       </td>
       <td>
        <%= scheduledProjects[key].projects %>
       </td>
       <td>
         <%= wipLimits[key].limit %>
        </td>
      </tr>
     </tbody>
    </table>
   </div>
  </div>
  <% if ( i === 2 || i === pods.length -1 ) { %>
 </div>
 <% } %>
 <% } %>
</main>
<% include ../../../partials/footer.ejs %>